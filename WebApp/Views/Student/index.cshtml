@{
    ViewData["Title"] = "Student List";
}
<h2>@ViewData["Title"]</h2>
<div class="row">
    <table class="table table-striped table-bordered table-hover">
        <thead> 
            <tr>
                <th>ID</th>
                <td>Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model) {
                <tr>
                    <td>@student.ID</td>
                    <td>@student.Name</td>
                </tr>
} </tbody>
    </table>
</div>